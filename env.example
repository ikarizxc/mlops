# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.
_AIRFLOW_WWW_USER_USERNAME=admin
_AIRFLOW_WWW_USER_PASSWORD=admin

# PostgreSQL Configuration
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=database
POSTGRES_PORT=5432

# MLflow Configuration
MLFLOW_SERVICE_PORT=5000

# AWS S3 Configuration (для MLflow artifacts)
# Для локальной разработки можно использовать MinIO или оставить пустыми
MLFLOW_S3_ENDPOINT_URL=your_s3_endpoint
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_DEFAULT_REGION=your_region
BUCKET_NAME=your_bucket_name

# Дополнительные настройки Airflow (опционально)
AIRFLOW_API_SERVER_PORT=8080
_PIP_ADDITIONAL_REQUIREMENTS= 